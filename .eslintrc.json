{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": [
    "plugin:prettier/recommended"
  ],
  "env": {
    "es6": true,
    "jest": true,
    "browser": true,
    "jquery": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "plugins": [],
  "rules": {
    "semi": [
      "error",
      "never"
    ]
  }
}
